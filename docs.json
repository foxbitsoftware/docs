{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Crunchforms Documentation",
  "colors": {
    "primary": "#264653",
    "light": "#fff",
    "dark": "#2A9D8F"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "faq",
              "form-config"
            ]
          },
          {
            "group": "Advanced",
            "pages": [
              "advanced/spam",
              "advanced/honeypot",
              "advanced/turnstile",
              "advanced/environments",
              "advanced/emails",
              "advanced/whitelabeling"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/submit"
            ]
          }
        ]
      }
